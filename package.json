{
  "name": "micro-frontend-starter",
  "version": "0.0.1",
  "description": "Template for starter of micro frontends",
  "main": "index.js",
  "scripts": {
    "bootstrap": "pnpm i",
    "dev": "cross-env VITE_MICRO_MODE=true tsx ./scripts/dev.ts",
    "server": "cross-env VITE_MICRO_MODE=true tsx ./scripts/server_dev.ts",
    "build:idea": "pnpm run build --app idea",
    "build:system": "pnpm run build --app system",
    "build:frontend": "pnpm run build --app frontend",
    "build": "cross-env VITE_MICRO_MODE=true tsx ./scripts/build.ts",
    "lint": "eslint --ext .js,.ts,.vue --fix ./packages/*/src",
    "prettier": "prettier --write --ignore-unknown '**/*'",
    "commit": "git-cz",
    "release": "npx bumpp --push --tag --commit 'release: v'",
    "prepare": "husky install",
    "docker:build": "docker build --progress=plain -t micro-frontend-starter .",
    "docker:up": "docker-compose up -d"
  },
  "keywords": [
    "micro-frontends",
    "qiankun",
    "vue",
    "react",
    "svelte"
  ],
  "author": "yugasun",
  "license": "MIT",
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "@types/crypto-js": "^4.1.1",
    "@types/sm-crypto": "^0.3.0",
    "@types/js-cookie": "^3.0.3",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.186",
    "@types/node": "^17.0.31",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "@ctrl/tinycolor": "^3.6.0",
    "bumpp": "^7.1.1",
    "commitizen": "^4.2.4",
    "commitlint-config-cz": "^0.13.3",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "eslint": "^8.36.0",
    "vite-plugin-eslint": "^1.8.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "execa": "^6.1.0",
    "fs-extra": "^10.1.0",
    "glob": "^8.0.3",
    "husky": "^8.0.1",
    "sass": "^1.59.3",
    "less": "^4.1.3",
    "less-plugin-import-node-modules": "^1.0.0",
    "lint-staged": "^12.4.1",
    "minimist": "^1.2.6",
    "picocolors": "^1.0.0",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "tsx": "^3.8.0",
    "typescript": "^4.7.4",
    "vite-plugin-dts": "^1.4.1",
    "vite": "^4.2.0",
    "vite-plugin-html": "^3.2.0",
    "vite-plugin-compression2": "^0.8.2",
    "vite-plugin-svgr": "^2.4.0",
    "vite-plugin-comlink": "^3.0.5",
    "vite-plugin-checker": "^0.5.6",
    "comlink": "^4.4.1",
    "autoprefixer": "^10.4.7",
    "compression": "^1.7.4",
    "postcss": "^8.4.14",
    "rollup": "^2.79.1",
    "js-cookie": "^3.0.1",
    "uuidv4": "^6.2.13",
    "jsencrypt": "^3.3.2",
    "sm-crypto": "^0.3.12",
    "crypto-js": "^4.1.1",
    "util": "^0.12.5",
    "@types/lodash-es": "^4.17.7",
    "lodash-es": "^4.17.21",
    "qiankun": "^2.10.3",
    "moment": "^2.29.4",
    "vite-plugin-qiankun": "^1.0.15"
  },
  "packageManager": "pnpm@7.8.0",
  "dependencies": {}
}
